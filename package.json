{
  "homepage": "https://gregorycannon.github.io/TetrisTrainer/",
  "name": "tetrisjs",
  "version": "1.0.0",
  "description": "tetris trainer",
  "main": "src/webclient/index.js",
  "scripts": {
    "deploy": "npm run make && gh-pages -d docs",
    "start-ui": "prettier src/webclient/*.js --write && webpack --progress --mode=development && webpack-dev-server",
    "start-server": "tsc && node built/src/server/app.js",
    "start": "node built/src/server/app.js",
    "make": "prettier src/*.js --write && webpack --progress --mode=production",
    "format": "prettier --write \"src/**/*.+(js|jsx|ts|json|css|md)\""
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^15.3.0",
    "gh-pages": "^3.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "fs": "0.0.1-security",
    "node-fetch": "^2.6.1",
    "prettier": "^2.0.5",
    "typescript": "^4.2.3"
  }
}
